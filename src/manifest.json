{
    "manifest_version": 2,
    "name": "CampFire",
    "description": "Plugin of CampFire API",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "options_page": "index.html",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}